extends layout

block content

  .container-fluid
  
    .row
      .col-xs-10.col-xs-offset-1
        h1
          =title
          
    .row
      .col-sm-6.col-sm-offset-3.col-xs-10.col-xs-offset-1
        .list-group
          each place in list.Places
            a.list-group-item(href='/place/'+place.id)
              h4.list-group-item-heading
                =place.name
              p.list-group-item-text
                =place.comment
